{
  "servers": {
    "telegram": {
      "command": "npx",
      "args": ["-y", "@chaindead/telegram-mcp"],
      "env": {
        "TG_APP_ID": "TG_APP_ID",
        "TG_API_HASH": "TG_API_HASH"
      }
    }
  ,
    "infojobs": {
      "env": {
        "INFOJOBS_CLIENT_ID": "TU_CLIENT_ID",
        "INFOJOBS_CLIENT_SECRET": "TU_CLIENT_SECRET",
        "INFOJOBS_REDIRECT_URI": "http://localhost:8080/callback"
      }
    }
  }
}